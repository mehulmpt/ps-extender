{
	"manifest_version": 3,
	"name": "Practice School Extender for BITS Pilani",
	"version": "2.4.0",
	"description": "Extends functionality of PSD interface by adding controls for sorting rows, backups, problem bank viewer",
	"icons": {
		"32": "icon.png",
		"48": "icon.png",
		"64": "icon.png",
		"128": "icon.png"
	},
	"background": {
		"service_worker": "script.js"
	},
	"permissions": [
		"activeTab",
		"scripting"
	],
	"action": {},
	"web_accessible_resources": [
		{
			"resources": [
				"script.js",
				"style.css",
				"icon.png",
				"manifest.json",
				"csp.js"
			],
			"matches": [
				"https://psd.bits-pilani.ac.in/*"
			]
		}
	]
}